<article class="tile">
	<a href="/songs/<%=song.id%>">
		<div class="container">
			<span class="title"><%=song.title%></span>
			<span class="sub-title"><%=song.artist%></span>
		</div>
		<div class="bottom">
			<% song.roles.each do |role| %>
				<i class="flaticon-<%= role.instrument%> <%= role.found %>"></i>
			<% end %>
		</div>
	</a>
</article>
